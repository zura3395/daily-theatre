			function unicodeEscape(str) {
				let code, pref = {1: '', 2: '', 3: '\\u0', 4: '\\u'};
				return str.replace(/\W/g, function(c) {
					return pref[(code = c.charCodeAt(0).toString(16)).length] + ((code.length<3)?c:code);
				});
			};
