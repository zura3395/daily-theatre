				canvas.fillStyle = c;
				canvas.globalCompositeOperation = "lighter";
				canvas.fillRect(0,0,150,150); //icon
				canvas.globalCompositeOperation = "destination-in";
				canvas.drawImage(base,0,0,150,150,0,0,150,150);
				if (url) {
					return cv.toDataURL("image/png");
				} else {
